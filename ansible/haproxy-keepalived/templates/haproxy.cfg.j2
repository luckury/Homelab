frontend kubernetes-frontend
  bind *:8443
  mode tcp
  option tcplog
  default_backend kubernetes-backend

backend kubernetes-backend
  option httpchk GET /healthz
  http-check expect status 200
  mode tcp
  option ssl-hello-chk
  balance roundrobin
    server master1 {{ cpn001 }}:6443 check fall 3 rise 2
    server master2 {{ cpn002 }}:6443 check fall 3 rise 2
    server master3 {{ cpn003 }}:6443 check fall 3 rise 2
